{
  "name": "c2c-vue",
  "version": "1.0.0",
  "description": "Concept to Clinic Vue.js",
  "author": "DrivenData <info@drivendata.org>",
  "private": true,
  "scripts": {
    "dev": "node build/dev-server.js",
    "start": "node build/dev-server.js",
    "build": "node build/build.js",
    "unit": "npm run unit:test; npm run unit:coverage",
    "unit:test": "NODE_PATH=src nyc ava",
    "unit:coverage": "nyc report --reporter=html",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs"
  },
  "nyc": {
    "include": [
      "src/**/*.{js,vue}"
    ],
    "exclude": [
      "test",
      "node_modules"
    ],
    "extension": [
      ".js",
      ".vue"
    ]
  },
  "ava": {
    "concurrency": 5,
    "failFast": true,
    "files": [
      "test/unit/specs/**/*.spec.js"
    ],
    "source": [
      "src/**/*.{js,vue}"
    ],
    "require": [
      "./test/unit/index.js"
    ],
    "babel": "inherit"
  },
  "dependencies": {
    "vue-resource": "^1.3.4",
    "vue": "^2.5.6"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "ava": "^0.23.0",
    "axios": "^0.16.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "browser-env": "^3.2.2",
    "chai": "^3.5.0",
    "chalk": "^2.0.1",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "cornerstone-core": "^2.0.0",
    "cornerstone-math": "^0.1.6",
    "cornerstone-tools": "^2.0.0",
    "cross-env": "^5.0.1",
    "cross-spawn": "^5.0.1",
    "css-loader": "^0.28.0",
    "cssnano": "^3.10.0",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-html": "^3.0.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.1",
    "express-http-proxy": "^1.0.6",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.11.1",
    "friendly-errors-webpack-plugin": "^1.1.3",
    "html-webpack-plugin": "^2.28.0",
    "html2canvas": "0.5.0-beta4",
    "http-proxy-middleware": "^0.17.3",
    "inject-loader": "^3.0.0",
    "jspdf": "^1.3.5",
    "nightwatch": "^0.9.12",
    "node-sass": "^4.5.3",
    "nyc": "^11.3.0",
    "opn": "^5.1.0",
    "optimize-css-assets-webpack-plugin": "^2.0.0",
    "ora": "^1.2.0",
    "require-extension-hooks": "^0.3.2",
    "require-extension-hooks-babel": "^0.1.1",
    "require-extension-hooks-vue": "^0.4.1",
    "rimraf": "^2.6.0",
    "sass-loader": "^6.0.6",
    "semver": "^5.3.0",
    "shelljs": "^0.7.6",
    "url-loader": "^0.5.8",
    "vue-draggable-resizable": "^1.5.3",
    "vue-loader": "^13.0.4",
    "vue-router": "^2.7.0",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.5.6",
    "vue-test-utils": "^1.0.0-beta.6",
    "vuex": "^3.0.1",
    "webpack": "^2.6.1",
    "webpack-bundle-analyzer": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.19.1",
    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
